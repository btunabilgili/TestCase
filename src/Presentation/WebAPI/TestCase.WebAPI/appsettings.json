{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "testCase": "Server=localhost;Port=5432;Database=testCase;User Id=postgres;Password=postgresql;",
    "hangfire": "Server=localhost;Port=5432;Database=hangfire;User Id=postgres;Password=postgresql;",
    "logs": "Server=localhost;Port=5432;Database=logs;User Id=postgres;Password=postgresql;"
  },
  "JWTSettings": {
    "Issuer": "TestCase.issuer.com",
    "Audience": "TestCase.audience.com",
    "Key": "MyRandomTooLongStringKey"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "ConnectionString": "Server=localhost;Port=5432;Database=logs;User Id=postgres;Password=postgresql;",
          "TableName": "logs",
          "AutoCreateSqlTable": true
        }
      }
    ]
  }
}
